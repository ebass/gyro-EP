    1.    !------------------------------------------
    2.    ! GKEIGEN_split_com.f90
    3.    !
    4.    ! PURPOSE:
    5.    !   Split the MPI communicator into
    6.    !   gkeigen_proc_mult separate instances
    7.    !   of GYRO_COMM_WORLD.
    8.    !------------------------------------------
    9.    
   10.    subroutine GKEIGEN_split_comm
   11.    
   12.      use gyro_globals
   13.      use mpi
   14.    
   15.      integer :: ierr
   16.    
   17.      !----------------------------------------------------------------
   18.      ! Split the communicator into gkeigen_proc_mult parallel instances
   19.      ! of gyro each calculating a fraction of the columns of the
   20.      ! gkeigen matrix.
   21.      !
   22.      ! j_proc_tot : processor rank in MPI_COMM_WORLD
   23.      ! i_proc     : processor rank in GYRO_COMM_WORLD
   24.      ! n_proc     : (total process)/gkeigen_proc_mult
   25.      !               number of processes in each GYRO_COMM_WORLD
   26.      !
   27.      ! If gkeigen_proc_mult > 1, the user must specify a processor 
   28.      ! number that is an integer N such that:
   29.      !
   30.      !        N = (n_proc)*(gkeigen_proc_mult)
   31.      ! and
   32.      !
   33.      !        n_nek = (n_proc)*M, where M is any integer.
   34.      !
   35.      if ((linsolve_method/=2) .AND. (gkeigen_proc_mult/=1)) then
   36.        gkeigen_proc_mult = 1
   37.        call send_line('gkeigen_proc_mult reset to 1 (non-GKEIGEN run)')
   38.      endif
   39.    
   40.      gkeigen_j_set = Mod(i_proc,gkeigen_proc_mult)
   41.    
   42.      call MPI_COMM_SPLIT(GYRO_COMM_WORLD,&
   43.           gkeigen_j_set,&
   44.           i_proc,&
   45.           GKEIGEN_J_SUBSET, &
   46.           i_err)
   47.      !
   48.      GYRO_COMM_WORLD = GKEIGEN_J_SUBSET
   49.    
   50.      !---------------------------------------------------------------
   51.      ! Reset process ranks if MPI_COMM_WORLD was split.
   52.      call MPI_COMM_RANK(GYRO_COMM_WORLD,i_proc,i_err)
   53.      call MPI_COMM_SIZE(GYRO_COMM_WORLD,n_proc,i_err)
   54.      !---------------------------------------------------------------
   55.    
   56.      call MPI_COMM_RANK(MPI_COMM_WORLD,j_proc_tot,i_err)
   57.      call MPI_COMM_SIZE(MPI_COMM_WORLD,n_proc_tot,i_err)
   58.    
   59.      ! Generate a separate communicator for all process sharing
   60.      ! the same value of i_proc for use in MPI_ALLTOALL.
   61.      call MPI_COMM_SPLIT(MPI_COMM_WORLD,&
   62.                          i_proc,&
   63.                          j_proc_tot,&
   64.                          GYRO_COMM_UNIPROC,&
   65.                          ierr)
   66.    
   67.      if (debug_flag == 1) then
   68.        call send_line('[GKEIGEN_split_comm done]')
   69.      endif
   70.    
   71.    end subroutine GKEIGEN_split_comm


