    1.    !---------------------------------------
    2.    ! GKEIGEN_catch_error.f90
    3.    !
    4.    ! PURPOSE:
    5.    !  Check for self consistency in all GKEIGEN
    6.    !  input parameters. All inconsistencies are 
    7.    !  checked before terminating.
    8.    !---------------------------------------
    9.    
   10.    subroutine GKEIGEN_catch_error
   11.    
   12.      use gyro_globals
   13.      use gyro_pointers
   14.      use GKEIGEN_globals
   15.    
   16.      logical :: l_terminate = .false.
   17.    
   18.      character (len=100) :: error_message
   19.    
   20.    10 format(A, I7, '.')
   21.    20 format(A, I7, A, I7, A)
   22.    30 format(A, I3, A)
   23.    
   24.    !-- Temporarily disable the seldom used matrix reading/writing.
   25.    !   When it comes, the new implementation will be based on
   26.    !   pre-made PETSc matrix reading/writing.
   27.    !
   28.      If ((gkeigen_matrixonly==1) .or. (gkeigen_mwrite_flag==1)) &
   29.        call send_line('Matrix writing and restart temporarily disabled.')
   30.    
   31.      If (gkeigen_matrixonly == 1) Then
   32.        call send_line('GKEIGEN_MATRIXONLY=1 means simulation will abort.')
   33.        l_terminate = .true.
   34.      EndIf
   35.      If (gkeigen_mwrite_flag == 1) Then
   36.       gkeigen_mwrite_flag = 0
   37.       call send_line('GKEIGEN_MWRITE_FLAG reset to 0.')
   38.      EndIf
   39.          
   40.      eigensolve_restart_flag = 0
   41.    !
   42.    !---------------------------------------
   43.    
   44.    !-------------------------------------------------
   45.    ! Check for consistent dimensions of the eigenvalue problem.
   46.    !
   47.      If (gkeigen_kspace_dim > h_length) Then
   48.        write(error_message,10) 'INCONSISTENT: GKEIGEN_KSPACE_DIM must be <= matrix dimension ', h_length
   49.        call send_line(error_message)
   50.        write(error_message,10) 'GKEIGEN_KSPACE_DIM reset to ', h_length
   51.        call send_line(error_message)
   52.        gkeigen_kspace_dim = h_length
   53.      EndIf
   54.      If (gkeigen_n_values > gkeigen_kspace_dim) Then
   55.        call send_line('INCONSISTENT: GKEIGEN_N_VALUES must be <= GKEIGEN_KSPACE_DIM.') 
   56.        write(error_message,10) 'GKEIGEN_N_VALUES reset to ', gkeigen_kspace_dim
   57.        call send_line(error_message)
   58.        gkeigen_n_values = gkeigen_kspace_dim
   59.      EndIf
   60.    
   61.    !--------------------------------------------------
   62.    
   63.    !--------------------------------------------------
   64.    ! Check for consistent processor counts. GKEIGEN must
   65.    ! have a uniform distribution of matrix elements across
   66.    ! processors. This implies several restrictions.
   67.    !
   68.      If (mod(h_length,n_proc_tot)/=0) Then
   69.        write(error_message,20) 'Matrix dimension (', h_length, &
   70.          ') must be an integer multiple of total processors (', n_proc_tot, ')'
   71.        call send_line(error_message)
   72.        l_terminate = .true.
   73.      EndIf
   74.    
   75.      If ((mod(n_proc_tot,gkeigen_proc_mult)/=0) .or. (h_length_loc*n_proc/=h_length)) Then
   76.        write(error_message,20) 'Error(s) in n_proc = Total processors (', n_proc_tot, &
   77.          ') / GKEIGEN_PROC_MULT (', gkeigen_proc_mult, '):'
   78.        call send_line(error_message)
   79.        l_terminate = .true.
   80.      EndIf
   81.      If (mod(n_proc_tot,gkeigen_proc_mult)/=0) &
   82.        call send_line('   * n_proc must be an integer.')
   83.      If (h_length_loc*n_proc/=h_length) Then
   84.        write(error_message,30) '   * n_proc must divide evenly into ', n_energy*n_lambda/2, &
   85.             ' (from velocity grid).'
   86.        call send_line(error_message)
   87.      EndIf
   88.    
   89.      If (l_terminate) call catch_error('ABORTING JOB (see above).')
   90.    
   91.    end subroutine
