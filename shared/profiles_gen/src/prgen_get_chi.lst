    1.    subroutine prgen_get_chi(nx,q,kappa,rmin,psi,rho,bref,arho)
    2.    
    3.      implicit none
    4.    
    5.      integer, intent(in) :: nx
    6.      real, intent(in), dimension(nx) :: q
    7.      real, intent(in), dimension(nx) :: kappa
    8.      real, intent(in), dimension(nx) :: rmin
    9.      real, intent(in), dimension(nx) :: psi
   10.      real, intent(inout), dimension(nx) :: rho
   11.      real, intent(inout) :: bref
   12.      real, intent(inout) :: arho
   13.    
   14.      real, dimension(:), allocatable :: chi_t
   15.      integer :: i 
   16.    
   17.      !---------------------------------------------------------
   18.      ! Compute rho by integrating d(chi_t) = q d(psi_p)
   19.      ! using the trapezoidal rule
   20.      !
   21.      ! NOTE: chi_t/bref)
   22.      !
   23.      allocate(chi_t(nx))  
   24.      !
   25.      chi_t(1) = 0.0
   26.      do i=2,nx
   27.         chi_t(i) = chi_t(i-1) + 0.5*(q(i)+q(i-1))*(psi(i)-psi(i-1))
   28.      enddo
   29.      !
   30.      ! Even though bref is arbitrary, choose physically sensible values 
   31.      ! rather than, say, bref=1.  This is exact in the limit B is constant 
   32.      ! and the flux surfaces are elliptical with fixed kappa.
   33.      ! 
   34.      bref = 2*chi_t(nx)/(kappa(nx)*rmin(nx)**2)
   35.      arho = sqrt(kappa(nx))*rmin(nx)
   36.      !
   37.      ! Normalized root of chi_t:
   38.      !
   39.      rho(:) = sqrt(chi_t(:)/chi_t(nx))
