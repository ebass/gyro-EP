    1.    !------------------------------------------------------
    2.    ! logint.f90
    3.    !
    4.    ! PURPOSE:
    5.    !  Given the (negative) logarithmic gradient f of a 
    6.    !  function fi:
    7.    !
    8.    !                     x*
    9.    !                     /
   10.    !  fi(x) = fi(x*)exp[ | f(y) dy ]
   11.    !                     /
   12.    !                     x
   13.    !  
   14.    !  obtain fi by trapezoidal integration.  It is assumed 
   15.    !  that fi(x*) is known.
   16.    !
   17.    ! NOTES:
   18.    !      f(1:n) : (input) gradients
   19.    !       fi(n) : (input) boundary condition
   20.    !   fi(1:n-1) : (output) interior values
   21.    !------------------------------------------------------
   22.    
   23.    subroutine logint(fi,f,r,n,i0)
   24.    
   25.      implicit none
   26.    
   27.      integer, intent(in) :: n
   28.      integer, intent(in) :: i0
   29.    
   30.      real, intent(inout), dimension(n) :: fi
   31.      real, intent(in), dimension(n) :: f
   32.      real, intent(in),dimension(n) :: r
   33.    
   34.      integer :: i
   35.    
   36.    
   37.      if (i0 > 1) then
   38.         do i=i0,2,-1
   39.            fi(i-1) = fi(i)*exp(0.5*(f(i)+f(i-1))*(r(i)-r(i-1)))
   40.         enddo
   41.      endif
   42.    
   43.      if (i0 < n) then
   44.         do i=i0+1,n
   45.            fi(i) = fi(i-1)*exp(-0.5*(f(i)+f(i-1))*(r(i)-r(i-1)))
   46.         enddo
   47.      endif
   48.    
   49.    end subroutine logint


