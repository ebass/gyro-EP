    1.    !---------------------------------------------------------------
    2.    ! cub_spline_deriv.f90
    3.    !
    4.    ! PURPOSE:
    5.    !  Take known points x(1:n),y(1:n) and perform cubic spline
    6.    !  interpolation at the node points xi(1:ni) to give yi(1:ni).
    7.    !
    8.    !  Use 'natural' cubic spline interpolation, where natural 
    9.    !  means the second derivative is zero at the boundaries.
   10.    !
   11.    !  INPUT  : x(1:n),y(1:n),n
   12.    !
   13.    !  OUTPUT : yp(1:ni)
   14.    !----------------------------------------------------------------
   15.    
   16.    subroutine cub_spline_deriv2(x,y,n,yp)
   17.    
   18.      !-------------------------------------------------------------
   19.      implicit none
   20.      !
   21.      integer :: i,ii
   22.      real :: x0 
   23.      !
   24.      integer, intent(in) :: n
   25.      real, intent(in), dimension(n) :: x,y
   26.      real, intent(inout), dimension(n) :: yp
   27.      !
   28.      ! LAPACK working variables
   29.      !
   30.      integer :: info
   31.      integer, dimension(n) :: ipiv 
   32.      real, dimension(n) :: c,z
   33.      real, dimension(n-1) :: zl,zu,h
   34.      real, dimension(n-2) :: zu2
   35.      real, dimension(n-1) :: b,d
   36.      !-------------------------------------------------------------
   37.    
   38.      !-------------------------------------------------------------
   39.      ! Define coefficients of spline matrix 
   40.      !
   41.      do i=1,n-1 
   42.         h(i)  = x(i+1)-x(i)
   43.         zl(i) = h(i)
   44.         zu(i) = h(i)
   45.      enddo
   46.      zl(n-1) = 0.0
   47.      zu(1)   = 0.0
   48.    
   49.      z(1) = 2*h(1)
   50.      do i=2,n-1
   51.         z(i) = 2.0*(h(i-1)+h(i))
   52.         c(i) = 3.0*((y(i+1)-y(i))/h(i)-(y(i)-y(i-1))/h(i-1))
   53.      enddo
   54.      z(n) = 2*h(n)
   55.      c(1) = 2*c(2)-c(3)
   56.      c(n) = 2*c(n-1)-c(n-2)
   57.      !-------------------------------------------------------------
   58.    
   59.      !-------------------------------------------------------------
   60.      ! Solve the system using LAPACK
   61.      !
   62.      call DGTTRF(n,zl,z,zu,zu2,ipiv,info)
   63.      call DGTTRS('N',n,1,zl,z,zu,zu2,ipiv,c,n,info)
   64.      !-------------------------------------------------------------
   65.    
   66.      !-------------------------------------------------------------
   67.      ! Find remaining polynomial coefficients:
   68.      !
   69.    !  c(n) = z(n)
   70.      do i=1,n-1
   71.         b(i) = (y(i+1)-y(i))/h(i)-h(i)*(2.0*c(i)+c(i+1))/3.0
   72.         d(i) = (c(i+1)-c(i))/(3.0*h(i))
   73.      enddo
   74.      !-------------------------------------------------------------
   75.    
   76.      !print *,c(:)
   77.      
   78.      !-------------------------------------------------------------
   79.      ! Using known polynomial coefficients, perform interpolation.
   80.      !
   81.      !  S(x) = y(i) + b(i) [x-x(i)] + c(i) [x-x(i)]^2 
   82.      !                                        + d(i) [x-x(i)]^3
   83.      !
   84.      ! and then compute derivative based on S'(x). 
   85.      i  = 1
   86.      ii = 1
   87.      do while (ii <= n)
   88.         x0 = x(ii)
   89.         if (x0 <= x(i+1)) then
   90.            yp(ii) = b(i)+(x0-x(i))*(2*c(i)+(x0-x(i))*3*d(i))
   91.            ii = ii+1
   92.         else
   93.            i = i+1
   94.         endif
   95.      enddo
   96.      !-------------------------------------------------------------
   97.    
   98.    end subroutine cub_spline_deriv2
