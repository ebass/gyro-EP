    1.    !------------------------------------------------------
    2.    ! linint.f90
    3.    !
    4.    ! PURPOSE:
    5.    !  Given the gradient f of a function fi:
    6.    !
    7.    !                   x
    8.    !                   /
    9.    !  fi(x) = fi(x*) + | f(y) dy 
   10.    !                   /
   11.    !                   x*
   12.    !  
   13.    !  obtain fi by trapezoidal integration.  It is assumed 
   14.    !  that fi(x*) is known, where x(i0)=x*.
   15.    !
   16.    ! NOTES:
   17.    !      f(1:n) : (input) gradients
   18.    !      fi(i0) : (input) boundary condition
   19.    !   fi(1:n-1) : (output) interior values
   20.    !------------------------------------------------------
   21.    
   22.    subroutine linint(fi,f,r,n,i0)
   23.    
   24.      implicit none
   25.    
   26.      integer, intent(in) :: n
   27.      integer, intent(in) :: i0
   28.    
   29.      real, intent(inout), dimension(n) :: fi
   30.      real, intent(in), dimension(n) :: f
   31.      real, intent(in),dimension(n) :: r
   32.    
   33.      integer :: i
   34.    
   35.    
   36.      if (i0 > 1) then
   37.         do i=i0,2,-1
   38.            fi(i-1) = fi(i)-0.5*(f(i)+f(i-1))*(r(i)-r(i-1))
   39.         enddo
   40.      endif
   41.    
   42.      if (i0 < n) then
   43.         do i=i0+1,n
   44.            fi(i) = fi(i-1)+0.5*(f(i)+f(i-1))*(r(i)-r(i-1))
   45.         enddo
   46.      endif
   47.    
   48.    end subroutine linint


