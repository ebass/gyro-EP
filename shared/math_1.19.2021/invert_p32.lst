    1.    !------------------------------------------------
    2.    ! invert_p32.f90
    3.    !
    4.    ! PURPOSE:
    5.    !  Calculate x given s, where:
    6.    !
    7.    !                         x
    8.    !                  2      /          -t
    9.    ! s = p32(x) = --------   | sqrt(t) e   dt
   10.    !              sqrt(pi)   /
   11.    !                         0
   12.    !
   13.    ! Speed is not esential, so we use bisection 
   14.    ! for robsutness.
   15.    !
