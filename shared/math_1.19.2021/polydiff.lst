    1.    !-----------------------------------------------
    2.    ! polydiff.f90 
    3.    !
    4.    ! PURPOSE:
    5.    !  Compute coefficients for arbitrary-order, 
    6.    !  equally-spaced, uncentered finite-difference 
    7.    !  derivatives.
    8.    !
    9.    ! NOTES:
   10.    !  The coefficients computed are:
   11.    !
   12.    !           1   n
   13.    !  f'(j) = --- Sum c(p) f(p)
   14.    !           h  p=0    
   15.    !
   16.    !  where h = x(j) - x(j-1) (and f(j) means f[x(j)]).
   17.    !       
   18.    ! 
   19.    !
   20.    !                   /   Prod   (j-m) , if j /= p
   21.    !          1  f(p)  |  m /= p,j
   22.    !  c(p) = --- ----  |        
   23.    !          h   [p]  |   Sum     Prod   (p-m) , if j=p
   24.    !                   \  i /= j  m /= p,i
   25.    !
   26.    !
   27.    ! [p] =  Prod  (p-i)
   28.    !       i /= p
   29.    !
   30.    ! REVISIONS
   31.    ! 11 Jan 01: jeff.candy@gat.com
   32.    !  Added this revision info; code originally written
   33.    !  in fall 2000.
   34.    !--------------------------------------------------------------
   35.    
   36.    subroutine polydiff(n,j,c,denom)
   37.    
   38.      implicit none
   39.    
   40.      integer, intent(in) :: n
   41.      integer, intent(in) :: j
   42.    
   43.      real, intent(inout), dimension(0:n) :: c
   44.      real, intent(inout) :: denom
   45.    
   46.      integer :: p,k,i,m
   47.    
   48.      real :: temp
   49.      real :: add
   50.    
   51.    
   52.      do p=0,n
   53.    
   54.         temp = 1.0
   55.         do k=0,n
   56.            if (k /= p) temp = temp*(p-k)
   57.         enddo
   58.         denom = temp
   59.    
   60.         temp = 1.0
   61.         if (j /= p) then
   62.    
   63.            do m=0,n
   64.               if (m /= p .and. m /= j) temp = temp*(j-m)
   65.            enddo
   66.    
   67.            c(p) = temp/denom
   68.    
   69.         else
   70.    
   71.            add = 0.0
   72.            do i=0,n
   73.               if (i /= j) then
   74.                  temp = 1.0
   75.                  do m=0,n
   76.                     if (m /= p .and. m /= i) temp = temp*(p-m)
   77.                  enddo
   78.                  add = add+temp
   79.               endif
   80.            enddo
   81.    
   82.            c(p) = add/denom
   83.    
   84.         endif
   85.    
   86.      enddo
   87.    
   88.    end subroutine polydiff


