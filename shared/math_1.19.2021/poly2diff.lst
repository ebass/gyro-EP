   28.    subroutine poly2diff(n,c)
   29.    
   30.      implicit none
   31.    
   32.      integer, intent(in) :: n
   33.    
   34.      real, intent(inout), dimension(-n:n) :: c
   35.    
   36.      integer :: p
   37.      integer :: q
   38.      integer :: i
   39.      integer :: j
   40.    
   41.      real :: prod
   42.    
   43.    
   44.      do i=-n,n
   45.     
   46.         c(i) = 0.0
   47.    
   48.         do p=-n,n
   49.    
   50.            if (p /= i) then
   51.    
   52.               do q=-n,n
   53.    
   54.                  if (q /= i .and. q /= p) then
   55.    
   56.                     prod = 1.0
   57.    
   58.                     do j=-n,n
   59.    
   60.                        if (j /= i .and. j /= q .and. j /= p) then
   61.    
   62.                        prod = -j*prod/(i-j)
   63.    
   64.                        endif
   65.    
   66.                     enddo ! j
   67.    
   68.                     c(i) = c(i)+prod/((i-p)*(i-q))
   69.    
   70.                  endif
   71.    
   72.               enddo ! q
   73.    
   74.            endif
   75.    
   76.         enddo ! p
   77.    
   78.      enddo ! i
   79.     
   80.    end subroutine poly2diff


