    1.    !-------------------------------------------------------------
    2.    ! gyro_initialize_timestep.f90
    3.    !
    4.    ! PURPOSE:
    5.    !  Set number of steps based on maximum simulation time
    6.    !  (time_max).  For single-mode runs, the run will be
    7.    !  terminated early if freq_err drops below a given 
    8.    !  criteria (freq_tol).
    9.    !
   10.    !  Also, set integrator parameters and initialize timers.
   11.    !-------------------------------------------------------------
   12.    
   13.    subroutine gyro_initialize_timestep
   14.    
   15.      use gyro_globals
   16.    
   17.      !------------------------
   18.      implicit none
   19.      !
   20.      real :: new_max
   21.      !------------------------
   22.    
   23.      !----------------------------------------------------
   24.      ! Mikkelsen's time-step tolerance function
   25.      !
   26.      ! Note: freq_err is updated, and freq_err test 
   27.      ! executed, ONLY for single-mode runs.
   28.      !
   29.      if (time_max < 0.0) then
   30.    
   31.         ! Initialize status 'good' but unconverged.
   32.    
   33.         call gyro_set_exit_status('unconverged',0)
   34.    
   35.         if (abs(time_max) > 1.0) then
   36.    
   37.            ! Example:
   38.            !
   39.            ! time_max=-1000.001 then error tolerance is 
   40.            ! 0.001 and time_max -> 1000.0
   41.    
   42.            new_max  = int(abs(time_max))
   43.            freq_tol = abs(time_max) - new_max
   44.            time_max = real(new_max)
   45.            freq_err = 1.0
   46.    
   47.    
   48.         else
   49.    
   50.            ! Example:
   51.            !
   52.            ! time_max=-0.001 then error tolerance is 
   53.            ! 0.001 and time_max -> 50.0
   54.    
   55.            freq_tol = abs(time_max)
   56.            freq_err = 1.0
   57.            time_max = 50.0
   58.    
   59.         endif
   60.    
   61.      else
   62.    
   63.         ! Initialize status:
   64.    
   65.         call gyro_set_exit_status('clean exit',0)
   66.    
   67.         freq_tol = 1e-10
   68.         freq_err = 1.0
   69.    
   70.      endif
   71.    
   72.      nstep = nint(time_max/dt)
   73.      !----------------------------------------------------
   74.    
   75.      !----------------------------------------------------
   76.      ! Compute integrator substep parameter:
   77.      !
   78.      if (integrator_method > 1 .and. electron_method == 2) then
   79.    
   80.         n_substep = integrator_method-1
   81.    
   82.      else 
   83.    
   84.         ! We will always have n_substep=0 for explicit 
   85.         ! integration.
   86.    
   87.         n_substep = 0
   88.    
   89.      endif
   90.    
   91.      a_SDIRK = 0.5
   92.      !----------------------------------------------------
   93.    
   94.      if (debug_flag == 1 .and. i_proc == 0) then
   95.         print *,'[gyro_initialize_timestep done]'
   96.      endif
   97.    
   98.    end subroutine gyro_initialize_timestep


