    1.    !-----------------------------------------------------
    2.    ! gyro_make_poissonaperp_matrix.f90
    3.    !
    4.    ! PURPOSE:
    5.    !  Define sparse form of Poisson-AmperePerp (PB) 
    6.    !  matrix and factorize using UMFPACK.
    7.    !-----------------------------------------------------
    8.    
    9.    subroutine gyro_make_poissonaperp_matrix
   10.    
   11.      use gyro_globals
   12.      use gyro_poissonaperp_private
   13.    
   14.      !------------------------
   15.      implicit none
   16.      !------------------------
   17.    
   18.      !----------------------------------------------------------------
   19.      ! Begin by doing a hand-count of the nonzero elements in 
   20.      ! the sparse PB matrix:
   21.      !
   22.      ! Row dimension of sparse PB submatrix
   23.      n_poissonaperp_row = 2*n_x*n_blend
   24.    
   25.      ! radial bandwidth
   26.      n_gyro = 2*m_gyro-i_gyro+1
   27.    
   28.      if (boundary_method == 1) then
   29.    
   30.         ! nonzero elements in n=0 PB matrix:
   31.         n_zero = 4*(n_x-1)*n_gyro*n_blend**2+2*n_x*n_blend 
   32.    
   33.         ! nonzero elements in n>0 PB matrix:
   34.         n_fini = 4*n_x*n_gyro*n_blend**2
   35.    
   36.      else
   37.    
   38.         ! nonzero elements in nonperiodic PB matrices:
   39.         n_zero = 4*(n_x*n_gyro-m_gyro*(m_gyro+1))*n_blend**2 ! MPP,MPB,MBP,MBB
   40.    
   41.         n_fini = n_zero
   42.    
   43.      endif
   44.    
   45.      !----------------------------------------------------------------
   46.    
   47.      if (n_1(in_1) == 0) then
   48.         n_poissonaperp = n_zero
   49.      else
   50.         n_poissonaperp = n_fini
   51.      endif
   52.    
   53.      lindx(4)  = 2*n_poissonaperp
   54.      lvalue(4) = n_poissonaperp
   55.    
   56.      allocate(m_poissonaperp(lvalue(4)))
   57.      allocate(indx_poissonaperp(lindx(4)))
   58.    
   59.      if (n_1(in_1) == 0 .and. boundary_method == 1) then
   60.         n_x_max  = n_x-1
   61.      else
   62.         n_x_max  = n_x
   63.      endif
   64.    
   65.      ! Poisson MPP matrix
   66.      allocate(ap_mm(n_x,-m_gyro:m_gyro-i_gyro,n_blend,n_blend))
   67.      if (electron_method /= 2) then
   68.    
   69.         ! Adiabatic electrons or ions: (2-R) phi
   70.    
   71.         call gyro_blend_poisson(1)
   72.    
   73.      else
   74.    
   75.         ! Kinetic electrons: (1-R) phi
   76.    
   77.         call gyro_blend_poisson(0)
   78.    
   79.      endif
   80.    
   81.      ! Ampere Perp MBB matrix, and the coupled matrices MBP and MPB
   82.      allocate(ab_mm(n_x,-m_gyro:m_gyro-i_gyro,n_blend,n_blend))
   83.      allocate(abp_mm(n_x,-m_gyro:m_gyro-i_gyro,n_blend,n_blend))
   84.      call gyro_blend_ampereperp
   85.      call gyro_make_jperp_elec
   86.    
   87.      if (sparse_method == 1) then
   88.         call gyro_sparse_solve_umfpack(n_poissonaperp,n_poissonaperp_row,4,0)
   89.      else
   90.         call gyro_sparse_solve_mumps(n_poissonaperp,n_poissonaperp_row,4,0)
   91.      endif
   92.    
   93.      !---------------------------------------------
   94.      ! These are large matrices and deallocation is
   95.      ! important:
   96.      !
   97.      deallocate(ap_mm)
   98.      deallocate(ab_mm)
   99.      deallocate(abp_mm)
  100.      !---------------------------------------------
  101.    
  102.      if (debug_flag == 1 .and. i_proc == 0) then
  103.         print *,'[gyro_make_poissonaperp_matrix done]'
  104.      endif
  105.    
  106.    end subroutine gyro_make_poissonaperp_matrix


