    1.    !------------------------------------------------------
    2.    ! gyro_write_efficiency.f90 [caller BigScience]
    3.    !
    4.    ! PURPOSE:
    5.    !  This subroutine computes and prints the parallel
    6.    !  distribution efficiency.
    7.    !------------------------------------------------------
    8.    
    9.    subroutine gyro_write_efficiency(datafile,io)
   10.    
   11.      use gyro_globals
   12.    
   13.      !--------------------------------------------------
   14.      implicit none
   15.      !
   16.      integer :: msplit
   17.      integer :: nv2
   18.      integer :: nlit
   19.      integer, external :: parallel_dim
   20.      real :: x_s
   21.      !
   22.      integer, intent(in) :: io
   23.      character (len=*), intent(in) :: datafile
   24.      !--------------------------------------------------
   25.    
   26.      if (output_flag /= 1) return
   27.    
   28.      if (i_proc == 0) then
   29.    
   30.         open(unit=io,file=datafile,status='replace')
   31.    
   32.         write(io,*) '---------- DISTRIBUTION EFFICIENCY -------------'
   33.         write(io,*) 'N_proc  NL iter  Eff(%)'
   34.         write(io,*) '------  -------  ------'
   35.         do i=n_n,n_n*n_energy*n_lambda,n_n
   36.    
   37.            nv2    = parallel_dim(n_energy*n_lambda,i/n_n)
   38.            msplit = 1+(n_stack*nv2-1)/n_n
   39.            nlit   = msplit*n_kinetic  
   40.            x_s = (n_stack*n_energy*n_lambda*n_n/real(i))/(msplit*n_n)
   41.            if (x_s >= 0.99999) then
   42.               write(io,10) i,nlit,x_s*100,'***'
   43.            else if (x_s >= 0.9) then
   44.               write(io,10) i,nlit,x_s*100,'**'
   45.            else if (x_s >= 0.8) then
   46.               write(io,10) i,nlit,x_s*100,'*'
   47.            else 
   48.               write(io,10) i,nlit,x_s*100
   49.            endif
   50.         enddo ! i
   51.    
   52.         close(io)
   53.    
   54.      endif
   55.    
   56.    10 format(t2,i6,t12,i4,t19,f6.2,1x,a)
   57.    
   58.    end subroutine gyro_write_efficiency


