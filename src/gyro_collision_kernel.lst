    1.    !---------------------------------------------------
    2.    ! gyro_collision_kernel.f90
    3.    !
    4.    ! PURPOSE:
    5.    !  Take a collision step using the pitch-angle 
    6.    !  scattering operator with the RBF method.  
    7.    !---------------------------------------------------
    8.    
    9.    subroutine gyro_collision_kernel(ic)
   10.    
   11.      use gyro_globals
   12.      use gyro_collision_private
   13.      use gyro_pointers
   14.      use math_constants
   15.    
   16.      !--------------------------------------------------------------
   17.      implicit none
   18.      !
   19.      integer, intent(in) :: ic
   20.      integer :: p,pp
   21.      complex, dimension(n_rbf) :: fc,fcp,cphase
   22.      !---------------------------------------------------------------
   23.    
   24.      p_ine_loc = 0
   25.    
   26.      do p_ine = 1+i_proc_1,n_ine_1,n_proc_1
   27.    
   28.         p_ine_loc = p_ine_loc+1
   29.    
   30.         i  = ine_i(p_ine)
   31.         ie = ine_e(p_ine)
   32.    
   33.    !$omp parallel do private(p,m) schedule(static)
   34.         do k=1,n_lambda
   35.            do m=1,n_stack
   36.               p = m + (k - 1)*n_stack
   37.               cphase(p) = exp(i_c*angp(i)*theta_t(i,k,m))
   38.               fc(p) = h_c(m,k,p_ine_loc)*cphase(p)
   39.            enddo
   40.         enddo
   41.    
   42.         !----------------------------------------------- 
   43.         ! This is essentially the full collision advance
   44.         !
   45.    !$omp parallel do private(pp) schedule(static)
   46.         do p=1,n_rbf
   47.            fcp(p) = 0.0
   48.            do pp=1,n_rbf
   49.               fcp(p) = fcp(p)+d_rbf(pp,p,p_ine_loc,ic)*fc(pp)
   50.            enddo
   51.         enddo
   52.         !----------------------------------------------- 
   53.    
   54.    !$omp parallel do private(p,m) schedule(static)
   55.         do k=1,n_lambda
   56.            do m=1,n_stack
   57.               p = m + (k - 1)*n_stack
   58.               h_c(m,k,p_ine_loc) = fcp(p)/cphase(p)
   59.            enddo
   60.         enddo
   61.    
   62.      enddo ! p_ine_loc
   63.    
   64.      if (debug_flag == 1 .and. i_proc == 0) then
   65.         print *,'[gyro_collision_kernel done]'
   66.      endif
   67.    
   68.    end subroutine gyro_collision_kernel


