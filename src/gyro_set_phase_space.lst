    1.    !-----------------------------------------------------
    2.    ! gyro_set_phase_space.f90
    3.    !
    4.    ! PURPOSE:
    5.    !  Combine the results of make_energy_grid and 
    6.    !  make_lambda_grid -- to make phase-space weight
    7.    !  w_p(ie,k).
    8.    !
    9.    !  Also, print various weights if verbose = 1.
   10.    !-------------------------------------------------------
   11.    
   12.    subroutine gyro_set_phase_space(datafile,io)
   13.    
   14.      use gyro_globals
   15.      use math_constants
   16.    
   17.      implicit none
   18.    
   19.      integer, intent(in) :: io
   20.      character (len=*), intent(in) :: datafile
   21.    
   22.      do ie=1,n_energy
   23.         do i=1,n_x
   24.            do k=1,n_lambda
   25.               ck = class(k)
   26.               w_p(ie,i,k) = w_energy(ie)*w_lambda(i,k)*d_tau(ck)
   27.            enddo
   28.         enddo
   29.      enddo
   30.    
   31.      if (i_proc == 0 .and. output_flag == 1) then
   32.    
   33.         open(unit=io,file=datafile,status='replace')
   34.    
   35.         ! Energy grid output
   36.    
   37.         write(io,'(a)') ' i    energy(i)    w_energy(i) '
   38.         write(io,'(a)') '--- ------------  ------------ '
   39.         do ie=1,n_energy
   40.            write(io,'(i2,3(2x,f12.10))') ie,energy(ie),w_energy(ie)
   41.         enddo
   42.    
   43.         ! Lambda-grid output
   44.    
   45.         do i=1,n_x,n_x-1
   46.    
   47.            write(io,*)
   48.            write(io,'(a)') ' k   tau(k)/2pi     s_lambda(k)   lambda(k)   w_lambda(k)'
   49.            write(io,'(a)') '--- ------------  ------------ ------------  -----------'
   50.            do k=1,n_lambda
   51.               write(io,'(i2,4(2x,f12.10))') & 
   52.                    k,1.0/(pi_2*omega(i,k)),s_lambda(k),lambda(i,k),w_lambda(i,k)
   53.               if (k == n_pass) write(io,'(a,t33,f12.10)') 'lambda-TP:',lambda_tp(i)
   54.               if (k == n_lambda) write(io,'(a,t33,f12.10)') 'lambda-MAX:',lambda_max(i) 
   55.            enddo
   56.    
   57.         enddo
   58.    
   59.         close(io)
   60.    
   61.      endif
   62.    
   63.    end subroutine gyro_set_phase_space


