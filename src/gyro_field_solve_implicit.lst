    1.    subroutine gyro_field_solve_implicit
    2.    
    3.      use gyro_globals
    4.      use gyro_maxwell_private
    5.    
    6.      !---------------------------------------------------
    7.      implicit none
    8.      !---------------------------------------------------
    9.    
   10.      call gyro_operators_on_h
   11.    
   12.      ! Generate RHS for field solve
   13.      do ix=1,n_field
   14.         call gyro_velocity_sum(ix)
   15.      enddo
   16.    
   17.      call gyro_timer_in('Field-implicit')
   18.    
   19.      if (sparse_method == 1) then
   20.         call gyro_sparse_solve_umfpack(n_maxwell,n_maxwell_row,3,1)
   21.      else
   22.         call gyro_sparse_solve_mumps(n_maxwell,n_maxwell_row,3,1)
   23.      endif
   24.    
   25.      call gyro_timer_out('Field-implicit')
   26.    
   27.      if (debug_flag == 1 .and. i_proc == 0) then
   28.         print *,'[gyro_field_solve_implicit done]'
   29.      endif
   30.    
   31.    end subroutine gyro_field_solve_implicit


