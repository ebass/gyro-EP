    1.    !-----------------------------------------------------
    2.    ! gyro_geometry_arrays.f90 [caller BigScience]
    3.    !
    4.    ! PURPOSE:
    5.    !  Definition of arrays which require calls to 
    6.    !  general geometry library.
    7.    !-----------------------------------------------------
    8.    
    9.    subroutine gyro_geometry_arrays
   10.    
   11.      use gyro_globals
   12.      use GEO_interface
   13.    
   14.      !--------------------------------------
   15.      implicit none
   16.      !--------------------------------------
   17.    
   18.    
   19.      do i=1,n_x
   20.    
   21.         call gyro_to_geo(i)
   22.    
   23.         do k=1,n_lambda
   24.            do m=1,n_stack
   25.    
   26.               call GEO_interp(theta_t(i,k,m))
   27.    
   28.               b0_t(i,k,m)      = GEO_b
   29.               g_theta_t(i,k,m) = GEO_g_theta
   30.               grad_r_t(i,k,m)  = GEO_grad_r
   31.               qrat_t(i,k,m)    = GEO_gq
   32.    
   33.               if (geo_gradbcurv_flag == 1) then
   34.    
   35.                  ! No explicit pressure effect.  
   36.    
   37.                  ! This is also known as the grad B -> curvature 
   38.                  ! rule, where we remark that curvature has no 
   39.                  ! pressure dependence (but grad B does!).
   40.    
   41.                  cos_t(i,k,m)   = GEO_gcos1
   42.                  cos_p_t(i,k,m) = 0.0
   43.    
   44.               else 
   45.    
   46.                  ! Retain full grad-B and curvature effects.
   47.    
   48.                  cos_t(i,k,m)   = GEO_gcos1+GEO_gcos2
   49.                  cos_p_t(i,k,m) = -GEO_gcos2
   50.    
   51.               endif
   52.    
   53.               bt_t(i,k,m)   = GEO_bt
   54.               bp_t(i,k,m)   = GEO_bp
   55.               bigr_t(i,k,m) = GEO_bigr
   56.    
   57.               captheta_t(i,k,m) = GEO_captheta
   58.               sin_t(i,k,m)      = GEO_gsin
   59.    
   60.               usin_t(i,k,m) = GEO_usin
   61.               ucos_t(i,k,m) = GEO_ucos
   62.    
   63.            enddo ! m
   64.         enddo ! k 
   65.    
   66.         do j=1,n_theta_plot
   67.    
   68.            call GEO_interp(theta_plot(j))
   69.    
   70.            b0_plot(i,j)      = GEO_b
   71.            g_theta_plot(i,j) = GEO_g_theta
   72.    
   73.         enddo ! j
   74.    
   75.      enddo !i
   76.    
   77.      if (debug_flag == 1 .and. i_proc == 0) then
   78.         print *,'[gyro_geometry_arrays done]'
   79.      endif
   80.    
   81.    end subroutine gyro_geometry_arrays


