    1.    !------------------------------------------------------------
    2.    ! make_MPI_grid.f90
    3.    !
    4.    ! PURPOSE:
    5.    !  This routine manages most of the MPI initialization chore.
    6.    !------------------------------------------------------------
    7.    
    8.    subroutine gyro_mpi_grid
    9.    
   10.      use mpi
   11.      use gyro_globals
   12.      use gyro_pointers
   13.    
   14.      !----------------------------------------
   15.      implicit none
   16.      !
   17.      integer, external :: parallel_dim
   18.      integer :: i_group_mumps
   19.      integer :: splitkey
   20.      !----------------------------------------
   21.    
   22.      !------------------------------------------------
   23.      ! Check for grid validity
   24.      !
   25.      if (modulo(n_proc,n_n) /= 0) then
   26.         call catch_error('ERROR: (GYRO) bad processor count.')
   27.      endif
   28.      !
   29.      if (linsolve_method == 3 .and. modulo(n_energy,n_proc) /= 0) then
   30.         call catch_error('ERROR: (GYRO) bad processor count for LINSOLVE_METHOD=3.')
   31.      endif
   32.      !------------------------------------------------
   33.    
   34.      !-------------------------------
   35.      ! Assign subgroup dimensions:
   36.      !
   37.      n_proc_2 = n_n
   38.      n_proc_1 = n_proc/n_n
   39.      n_n_1    = 1
   40.      !-------------------------------
   41.    
   42.      !------------------------------------------------
   43.      ! Local group indices:
   44.      !
   45.      i_group_1 = i_proc/n_proc_1
   46.      i_group_2 = modulo(i_proc,n_proc_1)
   47.      !
   48.      !------------------------------------------------
   49.    
   50.      !-----------------------------------------------------------
   51.      ! Split up GYRO_COMM_WORLD into groups and adjoint:
   52.      !
   53.      !             NEW_COMM_1  and  NEW_COMM_2
   54.      !
   55.      splitkey = i_proc
   56.    
   57.      call MPI_COMM_SPLIT(GYRO_COMM_WORLD,&
   58.           i_group_1,& 
   59.           splitkey,&
   60.           NEW_COMM_1, &
   61.           i_err)
   62.      if (i_err /= 0) then
   63.         call CATCH_ERROR('ERROR: (GYRO) NEW_COMM_1 not created')
   64.      endif
   65.    
   66.      ! Local adjoint Group number
   67.    
   68.      call MPI_COMM_SPLIT(GYRO_COMM_WORLD,&
   69.           i_group_2,& 
   70.           splitkey,&
   71.           NEW_COMM_2, &
   72.           i_err)
   73.      if (i_err /= 0) then
   74.         call CATCH_ERROR('ERROR: (GYRO) NEW_COMM_2 not created')
   75.      endif
   76.      !
   77.      call MPI_COMM_RANK(NEW_COMM_1,i_proc_1,i_err)
   78.      call MPI_COMM_RANK(NEW_COMM_2,i_proc_2,i_err)
   79.      !
   80.      !------------------------------------------------------------
   81.    
   82.      ! Dimensions used for SSUB library:
   83.      ! (note that n_proc_2 == n_n).
   84.    
   85.      nv1_SSUB    = n_x
   86.      nv2_SSUB    = parallel_dim(n_energy*n_lambda,n_proc_1)
   87.      msplit_SSUB = 1+(n_stack*nv2_SSUB-1)/n_n
   88.    
   89.      call SSUB_init(n_n,n_stack,nv1_SSUB,nv2_SSUB,NEW_COMM_2)  
   90.    
   91.      if (debug_flag == 1) then
   92.         print *,'----------------------------------------'
   93.         print '(t2,a,2(i3,1x))','i_proc,i_group_1 : ',i_proc,i_group_1
   94.         print *,'----------------------------------------'
   95.      endif
   96.    
   97.      !--------------------------------------------------------------
   98.      ! Create MUMPS_COMM if using MUMPS:
   99.      !
  100.      if (sparse_method == 2) then
  101.         i_group_mumps = i_group_2/(min(n_proc_1,n_mumps_max))
  102.         call MPI_COMM_SPLIT(NEW_COMM_1,&
  103.              i_group_mumps,& 
  104.              i_proc_1,&
  105.              MUMPS_COMM, &
  106.              i_err)
  107.         if (i_err /= 0) then
  108.            call CATCH_ERROR('ERROR: (GYRO) MUMPS_COMM not created')
  109.         endif
  110.      endif
  111.      !--------------------------------------------------------------  
  112.    
  113.      if (debug_flag == 1 .and. i_proc == 0) then
  114.         print *,'[gyro_mpi_grid done]'
  115.      endif
  116.    
  117.    end subroutine gyro_mpi_grid


