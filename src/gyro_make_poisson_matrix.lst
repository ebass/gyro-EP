    1.    !-----------------------------------------------------
    2.    ! gyro_make_poisson_matrix.f90
    3.    !
    4.    ! PURPOSE:
    5.    !  Define sparse form of Poisson matrix used for 
    6.    !  EXPLICIT field solve.
    7.    !-----------------------------------------------------
    8.    
    9.    subroutine gyro_make_poisson_matrix
   10.    
   11.      use gyro_globals
   12.      use math_constants
   13.      use gyro_poisson_private
   14.    
   15.      !------------------------
   16.      implicit none
   17.      !------------------------
   18.    
   19.      !----------------------------------------------------------------
   20.      ! Begin by doing a hand-count of the nonzero elements in 
   21.      ! the sparse Maxwell matrix:
   22.      !
   23.      ! ELECTROSTATIC
   24.    
   25.      ! Row dimension of sparse Poisson matrix
   26.      n_poisson_row = n_x*n_blend
   27.    
   28.      ! radial band width
   29.      n_gyro = 2*m_gyro-i_gyro+1
   30.    
   31.      if (boundary_method == 1) then
   32.    
   33.         ! nonzero elements in n=0 PP matrix:
   34.         n_zero = (n_x-1)*n_gyro*n_blend**2+n_x*n_blend
   35.    
   36.         ! nonzero elements in n>0 PP matrix:
   37.         n_fini = n_x*n_gyro*n_blend**2
   38.    
   39.      else
   40.    
   41.         ! nonzero elements in n=0 nonperiodic PP matrix:
   42.         n_zero = (n_x*n_gyro-m_gyro*(m_gyro+1))*n_blend**2 ! MPP
   43.    
   44.         ! same number of nonzeros in n>0 nonperiodic matrices:
   45.         n_fini = n_zero
   46.    
   47.      endif
   48.      !----------------------------------------------------------------
   49.    
   50.    
   51.      if (n_1(in_1) == 0) then
   52.         n_poisson = n_zero
   53.      else
   54.         n_poisson = n_fini
   55.      endif
   56.    
   57.      lindx(1)  = 2*n_poisson
   58.      lvalue(1) = n_poisson
   59.    
   60.      allocate(m_poisson(lvalue(1)))
   61.      allocate(indx_poisson(lindx(1)))
   62.    
   63.      if (n_1(in_1) == 0 .and.  boundary_method == 1) then
   64.         n_x_max  = n_x-1
   65.      else
   66.         n_x_max  = n_x
   67.      endif
   68.    
   69.      allocate(ap_mm(n_x,-m_gyro:m_gyro-i_gyro,n_blend,n_blend))
   70.    
   71.      if (electron_method /= 2) then
   72.    
   73.         ! Adiabatic electrons or ions: (2-R) phi
   74.    
   75.         call gyro_blend_poisson(1)
   76.    
   77.      else
   78.    
   79.         ! Kinetic electrons: (1-R) phi
   80.    
   81.         call gyro_blend_poisson(0)
   82.    
   83.      endif
   84.    
   85.      if (sparse_method == 1) then
   86.         call gyro_sparse_solve_umfpack(n_poisson,n_poisson_row,1,0)
   87.      else
   88.         call gyro_sparse_solve_mumps(n_poisson,n_poisson_row,1,0)
   89.      endif
   90.    
   91.      !--------------------------------------------------------
   92.      ! These are large matrices and deallocation is important:
   93.      !
   94.      deallocate(ap_mm)
   95.      !--------------------------------------------------------
   96.    
   97.      if (debug_flag == 1 .and. i_proc == 0) then
   98.         print *,'[gyro_make_poisson_matrix done]'
   99.      endif
  100.    
  101.    end subroutine gyro_make_poisson_matrix


