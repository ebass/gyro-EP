    1.    subroutine gyro_theta_grid
    2.    
    3.      use gyro_globals
    4.      use math_constants
    5.    
    6.      implicit none
    7.    
    8.      !--------------------------------------------------------
    9.      ! NOTE: 
   10.      !
   11.      !  (1) tau's go all the way around a CLOSED orbit.
   12.      !  (2) tau is NORMALIZED according to: 
   13.      !
   14.      !              max[tau(1)] = 1 (passing)
   15.      !              max[tau(2)] = 2 (trapped)
   16.      !
   17.      !                        / 
   18.      !      This ensures that | dtau = 1 for a given sign 
   19.      !                        / 
   20.      !      of velocity.
   21.      !
   22.      !  (3) n_theta_section is the number of thetas in an 
   23.      !      "orbit section".  An orbit section is that length 
   24.      !      required to do an orbit integral -- 1/2 the orbit
   25.      !      for passing, and 1/4 the orbit for trapped.
   26.      !   
   27.      !
   28.      !  (4) n_theta(1) and n_theta(2) are the number of 
   29.      !      different values of theta touched by an orbit
   30.      !      of given type.
   31.      !
   32.      ! Passing [ n_tau = n_theta ]:
   33.      !
   34.      n_theta(1) = 2*n_theta_section-2
   35.      n_tau(1)   = n_theta(1)
   36.      !
   37.      ! Trapped [ n_tau = 2(n_theta-1) ]:
   38.      !
   39.      n_theta(2) = 2*n_theta_section-1
   40.      n_tau(2)   = 2*n_theta(2)-2
   41.      !
   42.      ! ** Note that n_stack is the same for both 
   43.      ! types of particles:
   44.      !
   45.      !  n_stack_pass = 2*n_theta_pass = 4*n_theta_section-4
   46.      !  n_stack_trap = n_tau_trap = 4*n_theta_section-4
   47.      !
   48.      n_stack = n_tau(2)
   49.      !
   50.      ! These should be equal, despite separate calculations!
   51.      !
   52.      d_tau(1) = 1.0/n_tau(1)
   53.      d_tau(2) = 2.0/n_tau(2)
   54.      !-------------------------------------------------------
   55.    
   56.      d_theta = pi_2/n_blend
   57.    
   58.      ! Real theta grid (arbitrary size)
   59.      n_theta_int = 2*n_blend
   60.    
   61.      ! Dimension of RBF collision matrices
   62.      !
   63.      n_rbf = n_stack*n_lambda
   64.    
   65.      if (debug_flag == 1 .and. i_proc == 0) then
   66.         print *,'[gyro_theta_grid done]'
   67.      endif
   68.    
   69.    end subroutine gyro_theta_grid


