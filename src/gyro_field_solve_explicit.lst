    1.    !-----------------------------------------------------
    2.    ! gyro_field_solve_explicit.f90
    3.    !
    4.    ! PURPOSE:
    5.    !  This routine coordinates calling of the appropriate 
    6.    !  sequence of routines for solution of the EXPLICIT 
    7.    !  Maxwell equations.
    8.    !-----------------------------------------------------
    9.    
   10.    subroutine gyro_field_solve_explicit
   11.    
   12.      use gyro_globals
   13.    
   14.      implicit none
   15.    
   16.      call gyro_operators_on_h
   17.    
   18.      ! Velocity-space sums (RHS)
   19.      do ix=1,n_field
   20.         call gyro_velocity_sum(ix)
   21.      enddo
   22.    
   23.      call gyro_timer_in('Field-explicit')
   24.    
   25.      select case (n_field)
   26.    
   27.      case (1)
   28.    
   29.         if (sparse_method == 1) then
   30.            call gyro_sparse_solve_umfpack(n_poisson,n_poisson_row,1,1)
   31.         else
   32.            call gyro_sparse_solve_mumps(n_poisson,n_poisson_row,1,1)
   33.         endif
   34.    
   35.      case (2)
   36.    
   37.         if (sparse_method == 1) then
   38.            call gyro_sparse_solve_umfpack(n_poisson,n_poisson_row,1,1)
   39.            call gyro_sparse_solve_umfpack(n_ampere,n_ampere_row,2,1)
   40.         else
   41.            call gyro_sparse_solve_mumps(n_poisson,n_poisson_row,1,1)
   42.            call gyro_sparse_solve_mumps(n_ampere,n_ampere_row,2,1)
   43.         endif
   44.    
   45.      case (3)
   46.    
   47.         if (sparse_method == 1) then
   48.            call gyro_sparse_solve_umfpack(n_poissonaperp,n_poissonaperp_row,4,1)
   49.            call gyro_sparse_solve_umfpack(n_ampere,n_ampere_row,2,1)
   50.         else
   51.            call gyro_sparse_solve_mumps(n_poissonaperp,n_poissonaperp_row,4,1)
   52.            call gyro_sparse_solve_mumps(n_ampere,n_ampere_row,2,1)
   53.         endif
   54.    
   55.      end select
   56.    
   57.      call gyro_timer_out('Field-explicit')
   58.    
   59.      call gyro_field_interpolation
   60.    
   61.    end subroutine gyro_field_solve_explicit


