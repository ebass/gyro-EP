    1.    !----------------------------------------------------------
    2.    ! gyro_banana_s2lambda.f90
    3.    !
    4.    ! PURPOSE:
    5.    !  Given s (integral of tau), calculate lambda.
    6.    ! 
    7.    ! NOTES:
    8.    !  Speed not esential, so use bisection for stability.
    9.    !----------------------------------------------------------
   10.    
   11.    subroutine gyro_banana_s2lambda(s,lambda)
   12.    
   13.      use gyro_banana_private
   14.    
   15.      !-----------------------------------------------------------
   16.      implicit none
   17.      !
   18.      real, parameter :: eps_lambda = 1e-9
   19.      real, intent(in) :: s
   20.      real, intent(inout) :: lambda
   21.      real :: dlambda
   22.      real :: s1
   23.      real :: s2
   24.      real :: residual
   25.      !-----------------------------------------------------------
   26.    
   27.      residual = 1.0
   28.    
   29.      dlambda = lambda_max/2.0-eps_lambda
   30.      lambda  = lambda_max/2.0
   31.    
   32.      do while (residual > eps_lambda)
   33.    
   34.         call gyro_banana_integrate_tau(lambda,s1)
   35.         call gyro_banana_integrate_tau(lambda+dlambda,s2)
   36.    
   37.         dlambda = 0.5*dlambda
   38.    
   39.         if (s1 <= s .and. s2 >= s) then
   40.            lambda = lambda+dlambda
   41.         else
   42.            lambda = lambda-dlambda
   43.         endif
   44.    
   45.         residual = abs(s2-s1)
   46.    
   47.      enddo
   48.    
   49.    end subroutine gyro_banana_s2lambda


