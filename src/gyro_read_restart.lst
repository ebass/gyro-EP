   32.      !---------------------------------------------------
   33.    
   34.      io = io_restart
   35.    
   36.      !---------------------------------------------------------------------
   37.      ! Set the step number to zero for gyro_write_timedata, even if 
   38.      ! this is a restart:
   39.      !
   40.      step = 0
   41.      !---------------------------------------------------------------------
   42.    
   43.      !---------------------------------------------------------------------
   44.      ! Check to see if we are asking for a restart when there is no 
   45.      ! restart data available:
   46.      !
   47.      if (i_proc == 0) then
   48.    
   49.         open(unit=io,&
   50.              file=trim(path)//file_tag_restart(restart_new_flag),&
   51.              status='old',iostat=i_err)
   52.         close(io)
   53.    
   54.         if (i_err /= 0 .and. restart_method > 0) then
   55.            call send_message(&
   56.                 'INFO: Restart data not available.  Reseting restart_method.')
   57.            restart_method = 0
   58.         endif
   59.    
   60.      endif
   61.      !---------------------------------------------------------------------
   62.    
   63.      ! Ensure sync.
   64.      call MPI_BCAST(restart_method,1,MPI_DOUBLE_PRECISION,0,GYRO_COMM_WORLD,i_err)
   65.    
   66.      select case(restart_method)
   67.    
   68.      case (-1)
   69.    
   70.         !------------------------
   71.         ! Ignore restart facility
   72.         !------------------------
   73.    
   74.         call gyro_initial_condition
   75.    
   76.         t_current = 0.0
   77.         i_restart = 0
   78.         data_step = 0
   79.    
   80.      case (0)
   81.    
   82.         !---------------------
   83.         ! New simulation block
   84.         !---------------------
   85.    
   86.         call gyro_initial_condition
   87.    
   88.         t_current = 0.0
   89.         i_restart = 0
   90.         data_step = 0
   91.    
   92.         ! i_restart is always tagged to most recent output 
   93.         ! files.  If late_restart = 0, then we want 1-i_restart
   94.    
   95.      case (1,2,3)
   96.    
   97.         !-------------------------------------------
   98.         ! Restart block
   99.         !
  100.         ! 1=restart, continue writing restart files
  101.         ! 2=restart, but do not write restart files
  102.         ! 3=restart, set t=0
  103.         !-------------------------------------------
  104.    
  105.         ! Get restart values from last run
  106.    
  107.         if (i_proc == 0) then
  108.    
  109.            open(unit=io,&
  110.                 file=trim(path)//file_tag_restart(restart_new_flag),&
  111.                 status='old')
  112.    
  113.            read(io,*) data_step
  114.            read(io,fmtstr) t_current
  115.            read(io,*) n_proc_old
  116.            read(io,*) i_restart
  117.            close(io)
  118.    
  119.         endif
  120.    
  121.         call MPI_BCAST(data_step,&
  122.              1,MPI_INTEGER,0,GYRO_COMM_WORLD,i_err)
  123.    
  124.         call MPI_BCAST(t_current,&
  125.              1,MPI_DOUBLE_PRECISION,0,GYRO_COMM_WORLD,i_err)
  126.    
  127.         call MPI_BCAST(n_proc_old,&
  128.              1,MPI_INTEGER,0,GYRO_COMM_WORLD,i_err)
  129.    
  130.         call MPI_BCAST(i_restart,&
  131.              1,MPI_INTEGER,0,GYRO_COMM_WORLD,i_err)
  132.    
  133.         ! Reset time if running TGYRO local method or using "gyro -start init"
  134.         if (transport_method == 2 .or. restart_method == 3) then
  135.            t_current = 0.0
  136.            data_step = 0
  137.         endif
  138.    
  139.         ! Trap error for incorrect number of processors
  140.         if (n_proc_old /= n_proc) then 
  141.            call catch_error('ERROR: (GYRO) Processor number changed.')
  142.         endif
  143.    
  144.         ! Determine which file to read
  145.    
  146.         filemode = IOR(MPI_MODE_RDWR,MPI_MODE_CREATE)
  147.         disp     = 0
  148.    
  149.         if (huge_restart == 0) then
  150.    
  151.            call MPI_INFO_CREATE(finfo,i_err)
  152.    
  153.            call MPI_FILE_OPEN(GYRO_COMM_WORLD,&
  154.                 trim(path)//file_restart(i_restart),&
  155.                 filemode,&
  156.                 finfo,&
  157.                 fhv,&
  158.                 i_err)
  159.    
  160.            call MPI_FILE_SET_VIEW(fhv,&
  161.                 disp,&
  162.                 MPI_COMPLEX16,&
  163.                 MPI_COMPLEX16,&
  164.                 'native',&
  165.                 finfo,&
  166.                 i_err)
  167.    
  168.            offset1 = size(h)*i_proc
  169.    
  170.            call MPI_FILE_READ_AT(fhv,&
  171.                 offset1,&
  172.                 h,&
  173.                 size(h),&
  174.                 MPI_COMPLEX16,&
  175.                 fstatus,&
  176.                 i_err)
  177.    
  178.            call MPI_FILE_CLOSE(fhv,i_err)
  179.    
  180.         else
  181.    
  182.            do is=1,n_kinetic
  183.               i_tag = achar(is-1+iachar("0"))
  184.    
  185.               call MPI_INFO_CREATE(finfo,i_err)
  186.    
  187.               call MPI_FILE_OPEN(GYRO_COMM_WORLD,&
  188.                    trim(path)//file_restart(i_restart)//i_tag,&
  189.                    filemode,&
  190.                    finfo,&
  191.                    fhv,&
  192.                    i_err)
  193.    
  194.               call MPI_FILE_SET_VIEW(fhv,&
  195.                    disp,&
  196.                    MPI_COMPLEX16,&
  197.                    MPI_COMPLEX16,&
  198.                    'native',&
  199.                    finfo,&
  200.                    i_err)
  201.    
  202.               offset1 = size(h(:,:,:,is))*i_proc
  203.    
  204.               call MPI_FILE_READ_AT(fhv,&
  205.                    offset1,&
  206.                    h(:,:,:,is),&
  207.                    size(h(:,:,:,is)),&
  208.                    MPI_COMPLEX16,&
  209.                    fstatus,&
  210.                    i_err)
  211.    
  212.               call MPI_FILE_CLOSE(fhv,i_err)
  213.            enddo
  214.         endif
  215.    
  216.      case default
  217.    
  218.         call catch_error('ERROR: (GYRO) Bad value for restart_method.')
  219.    
  220.      end select
  221.    
  222.      ! ** Regenerate fields:
  223.    
  224.      call gyro_field_solve_explicit
  225.    
  226.      if (debug_flag == 1 .and. i_proc == 0) then
  227.         print *,'[gyro_read_restart (MPI-IO) done]'
  228.      endif
  229.    
  230.    end subroutine gyro_read_restart


