    9.    subroutine gyro_read_restart
   10.    
   11.      use mpi
   12.      use gyro_globals
   13.      use gyro_pointers
   14.    
   15.      !---------------------------------------------------
   16.      ! Local variables:
   17.      !
   18.      implicit none
   19.      !
   20.      integer :: n_proc_old
   21.      integer :: io
   22.      character (len=1) :: i_tag
   23.      !
   24.      ! Required for MPI-IO: 
   25.      !
   26.      integer :: filemode
   27.      integer :: finfo
   28.      integer :: fhv
   29.      integer :: fstatus(MPI_STATUS_SIZE)
   30.      integer(kind=MPI_OFFSET_KIND) :: disp
