   19.    ! block partition do-loop indices i1 to i2 
   20.    ! for thread : mythd = 0, ..., nthds - 1
   21.    ! using a method that is as balanced as possible
   22.    !------------------------------------------------
   23.    
   24.    subroutine looplimits(mythd,nthds,i1,i2,ibeg,iend)
   25.    
   26.      implicit none
   27.      integer, intent(in) :: mythd, nthds, i1, i2
   28.      integer, intent(out) :: ibeg, iend
   29.      integer :: nwork, chunk, extra, ntcut  
   30.    
   31.      nwork = i2 - i1 + 1
   32.      chunk = nwork/nthds
   33.      extra = nwork - nthds*chunk
   34.      ntcut = nthds - extra
   35.      if (mythd < ntcut) then
   36.        ibeg = i1 + mythd*chunk
   37.        iend = ibeg + chunk - 1
   38.      else
   39.        ibeg = i1 + ntcut*chunk + (mythd - ntcut)*(chunk + 1)
   40.        iend = ibeg + chunk
   41.      end if
   42.      if (iend > i2) iend = i2
   43.    
   44.    end subroutine looplimits
