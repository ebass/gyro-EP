    1.    !-----------------------------------------------------
    2.    ! gyro_make_ampere_matrix.f90
    3.    !
    4.    ! PURPOSE:
    5.    !  Define sparse form of Ampere matrix (also used 
    6.    !  for A_par advance in the collision step) and 
    7.    !  factorize using UMFPACK.
    8.    !-----------------------------------------------------
    9.    
   10.    subroutine gyro_make_ampere_matrix
   11.    
   12.      use gyro_globals
   13.      use gyro_collision_private
   14.      use math_constants
   15.    
   16.      !------------------------
   17.      implicit none
   18.      !-----------------------
   19.    
   20.      ! Row dimension of sparse Ampere matrix
   21.      n_ampere_row = n_x*n_blend
   22.    
   23.      ! radial band width
   24.      n_x2 = 2*mg_dx-ig_dx+1
   25.    
   26.      if (boundary_method == 1) then
   27.    
   28.         ! nonzero elements in n=0 Ampere matrix:
   29.         n_zero = (n_x-1)*n_x2*n_blend**2+n_x*n_blend
   30.    
   31.         ! nonzero elements in n>0 Ampere matrix:
   32.         n_fini = n_x*n_x2*n_blend**2
   33.    
   34.      else
   35.    
   36.         ! nonzero elements in nonperiodic Ampere matrices:
   37.         n_zero = (n_x*n_x2-mg_dx*(mg_dx+1))*n_blend**2
   38.    
   39.         ! same number of nonzeros in n>0 nonperiodic matrices:
   40.         n_fini = n_zero
   41.    
   42.      endif
   43.    
   44.      if (n_1(in_1) == 0) then
   45.         n_ampere = n_zero
   46.      else
   47.         n_ampere = n_fini
   48.      endif
   49.    
   50.      lindx(2)  = 2*n_ampere
   51.      lvalue(2) = n_ampere
   52.    
   53.      allocate(m_ampere(lvalue(2)))
   54.      allocate(indx_ampere(lindx(2)))
   55.    
   56.      if (n_1(in_1) == 0 .and. boundary_method == 1) then
   57.         n_x_max  = n_x-1
   58.      else
   59.         n_x_max  = n_x
   60.      endif
   61.    
   62.      ! Ampere MAA matrix
   63.      allocate(aa_mm(n_x,-mg_dx:mg_dx-ig_dx,n_blend,n_blend))
   64.      call gyro_blend_ampere
   65.      call gyro_make_jpar_elec(0)
   66.    
   67.      if (sparse_method == 1) then
   68.         call gyro_sparse_solve_umfpack(n_ampere,n_ampere_row,2,0)
   69.      else
   70.         call gyro_sparse_solve_mumps(n_ampere,n_ampere_row,2,0)
   71.      endif
   72.    
   73.      !---------------------------------------------
   74.      ! These are large matrices and deallocation is
   75.      ! important:
   76.      !
   77.      deallocate(aa_mm)
   78.      !---------------------------------------------
   79.    
   80.      if (debug_flag == 1 .and. i_proc == 0) then
   81.         print *,'[gyro_make_ampere_matrix done]'
   82.      endif
   83.    
   84.    end subroutine gyro_make_ampere_matrix


