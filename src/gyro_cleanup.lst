    1.    !-----------------------------------------------------------
    2.    ! gyro_cleanup.f90
    3.    !
    4.    ! PURPOSE:
    5.    !  Deallocate and clean up.
    6.    !-----------------------------------------------------------
    7.    
    8.    subroutine gyro_cleanup
    9.    
   10.      use mpi
   11.      use gyro_globals
   12.      use gyro_interface
   13.    
   14.      implicit none
   15.    
   16.      ! No need for cleanup in test mode
   17.      if (gyrotest_flag == 1) return
   18.    
   19.      if (allocated(r)) then
   20.    
   21.         ! GYRO arrays
   22.         call gyro_alloc_profile_sim(0)
   23.         call gyro_alloc_big(0)
   24.         call gyro_alloc_orbit(0)
   25.         call gyro_alloc_velocity(0)
   26.         call gyro_alloc_distrib(0)
   27.         if (nonlinear_flag == 1) call gyro_alloc_nl(0)
   28.    
   29.         ! Sparse field arrays 
   30.         if (allocated(m_poisson)) deallocate(m_poisson)
   31.         if (allocated(indx_poisson)) deallocate(indx_poisson)
   32.         if (allocated(m_ampere)) deallocate(m_ampere)
   33.         if (allocated(indx_ampere)) deallocate(indx_ampere)
   34.         if (allocated(m_maxwell)) deallocate(m_maxwell)
   35.         if (allocated(indx_maxwell)) deallocate(indx_maxwell)
   36.         if (allocated(m_poissonaperp)) deallocate(m_poissonaperp)
   37.         if (allocated(indx_poissonaperp)) deallocate(indx_poissonaperp)
   38.    
   39.         ! Blending coefficients
   40.         call BLEND_cleanup
   41.    
   42.         call MPI_COMM_FREE(NEW_COMM_1,i_err)
   43.         call MPI_COMM_FREE(NEW_COMM_2,i_err)
   44.         if (sparse_method == 2) call MPI_COMM_FREE(MUMPS_COMM,i_err)
   45.    
   46.      endif
   47.    
   48.    end subroutine gyro_cleanup


