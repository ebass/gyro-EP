    8.    program gyro
    9.    
   10.      use mpi
   11.      use gyro_globals
   12.      use ompdata
   13.    
   14.      !-----------------------------------------------------------------
   15.      implicit none
   16.      !
   17.      integer :: ierr
   18.      integer :: supported
   19.    !  integer, external :: omp_get_max_threads  ! OMP 1.19.2021
   20.      !-----------------------------------------------------------------
   21.    
   22.      !----------------------------------------------------------------
   23.      ! Query OpenMP for threads
   24.      !
   25.    !  n_omp = omp_get_max_threads()  ! OMP 1.19.2021
   26.      n_omp = 1    ! OMP 1.19.2021
   27.      !-----------------------------------------------------------------
   28.    
   29.      !-----------------------------------------------------------------
   30.      ! Initialize MPI_COMM_WORLD communicator, including support for 
   31.      ! funneled threading (needed if OpenMP is enabled).
   32.      !
   33.      if (n_omp > 1) then
   34.         call MPI_INIT_THREAD(MPI_THREAD_FUNNELED,supported,ierr)
   35.         if (supported < MPI_THREAD_FUNNELED) then
   36.            call catch_error('ERROR: (GYRO) Multi-threaded MPI not supported.')
   37.         endif
   38.      else 
   39.         call MPI_INIT_THREAD(MPI_THREAD_SINGLE,supported,ierr)
   40.      endif
   41.      !-----------------------------------------------------------------
   42.    
   43.      !-----------------------------------------------------------------
   44.      ! Path is cwd:
   45.      !
   46.      path= './'
   47.      !-----------------------------------------------------------------
   48.    
   49.      !-----------------------------------------------------------------
   50.      ! Query MPI for dimensions
   51.      !
   52.      GYRO_COMM_WORLD = MPI_COMM_WORLD
   53.      !
   54.      ! Query rank and size
   55.      !
   56.      call MPI_COMM_RANK(GYRO_COMM_WORLD,i_proc,i_err)
   57.      call MPI_COMM_SIZE(GYRO_COMM_WORLD,n_proc,i_err)
   58.      !-----------------------------------------------------------------
   59.    
   60.      !-----------------------------------------------------------------
   61.      ! Standard standalone operation
   62.      transport_method = 1
   63.      !
   64.      call gyro_read_input
   65.      call gyro_read_input_extra
   66.      !
   67.      !-----------------------------------------------------------------
   68.      ! initialize OpenMP runtime parameters including block indices
   69.      call gyro_init_ompdata()
   70.      !
   71.      !----------------------------------------------------------------
   72.      ! Split the communicator if using GKEIGEN added parallelization.
   73.      call GKEIGEN_split_comm
   74.      !---------------------------------------------------------------
   75.    
   76.      ! Run gyro.
   77.      call gyro_do
   78.      call send_line('STATUS: '//gyro_exit_message)
   79.    
   80.      call MPI_FINALIZE(ierr)
   81.    
   82.    end program gyro


