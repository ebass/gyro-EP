    4.    ! PURPOSE:
    5.    !  Create and destroy nonlinear work arrays.
    6.    !
    7.    ! NOTES:
    8.    !  flag=0: deallocate
    9.    !  flag=1: allocate
   10.    !----------------------------------------------------------
   11.    
   12.    subroutine gyro_alloc_nl(flag)
