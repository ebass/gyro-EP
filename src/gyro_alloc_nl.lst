   14.      use gyro_globals 
   15.      use gyro_nl_private
   16.    
   17.      integer, intent(in) :: flag
   18.    
   19.      if (flag == 1 .and. allocated(n_p)) then
   20.         if (i_proc == 0) then
   21.            print *,'WARNING: already allocated arrays in gyro_alloc_nl'
   22.         endif
