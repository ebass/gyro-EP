   24.      endif
   25.      if (flag == 0 .and. .not.allocated(n_p)) then
   26.         if (i_proc == 0) then
   27.            print *,'WARNING: cannot deallocate arrays in gyro_alloc_nl'
   28.         endif
   29.         return
   30.      endif
   31.    
   32.      if (flag == 1) then
   33.    
   34.         if (nonlinear_flag == 1) then
   35.            ! Shared allocations
   36.            allocate(n_p(-n_max:n_max))
