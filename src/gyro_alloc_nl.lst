   53.         if (allocated(v_fft3)) deallocate(v_fft3)
   54.         if (allocated(vt_fft3)) deallocate(vt_fft3)
   55.    
   56.         ! Special ESSL deallocations
   57.         if (allocated(aux1_dcrft)) deallocate(aux1_dcrft)
   58.         if (allocated(aux2_dcrft)) deallocate(aux2_dcrft)
   59.         if (allocated(aux1_drcft)) deallocate(aux1_drcft)
   60.         if (allocated(aux2_drcft)) deallocate(aux2_drcft)
