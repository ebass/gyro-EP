    1.    !---------------------------------------------------------------
    2.    ! SSUB_init.f90
    3.    !
    4.    ! PURPOSE:
    5.    !  Initialize SSUB library.
    6.    !
    7.    ! NOTES:
    8.    !  This scheme is most efficient when n_j is a multiple of n_n.  
    9.    !---------------------------------------------------------------
   10.    
   11.    subroutine SSUB_init(nn_i,nj_i,nv1_i,nv2_i,COMM)
   12.    
   13.      use mpi
   14.      use SSUB_private
   15.    
   16.      !-------------------------------------------
   17.      implicit none
   18.      !
   19.      integer, intent(in) :: nn_i
   20.      integer, intent(in) :: nj_i
   21.      integer, intent(in) :: nv1_i
   22.      integer, intent(in) :: nv2_i
   23.      integer, intent(in) :: COMM
   24.      !-------------------------------------------
   25.    
   26.    
   27.      SSUB_COMM = COMM
   28.    
   29.      !----------------------------------------------
   30.      ! Get rank and number of processors from 
   31.      ! SSUB_COMM (which is assumed to already exist).
   32.      !
   33.      call MPI_COMM_RANK(SSUB_COMM,i_proc,i_err)
   34.      call MPI_COMM_SIZE(SSUB_COMM,n_proc,i_err)
   35.      !
   36.      !-----------------------------------------------
   37.    
   38.      nn  = nn_i
   39.      nj  = nj_i
   40.      nv1 = nv1_i
   41.      nv2 = nv2_i
   42.    
   43.      jsplit = 1+(nj*nv2-1)/nn
   44.    
   45.    end subroutine SSUB_init


