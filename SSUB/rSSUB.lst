   15.    !  New blocking scheme.
   16.    !---------------------------------------------
   17.    
   18.    subroutine rSSUB(xt,x_OUT)
   19.    
   20.      use mpi
   21.      use SSUB_private
   22.    
   23.      !-------------------------------------------------------
   24.      implicit none
   25.      !
   26.      integer :: j,p,s
   27.      complex, intent(in), dimension(nv1,jsplit,nn) :: xt
   28.      complex, intent(inout), dimension(nj,nv1,nv2) :: x_OUT
   29.      complex, dimension(nv1,jsplit*nn) :: x
   30.      !-------------------------------------------------------  
   31.    
   32.    
   33.      call MPI_ALLTOALL(xt, &
   34.           nv1*jsplit, &
   35.           MPI_DOUBLE_COMPLEX, &
   36.           x, &
   37.           nv1*jsplit, &
   38.           MPI_DOUBLE_COMPLEX, &
   39.           SSUB_COMM, &
   40.           i_err)
   41.    
   42.    !$omp parallel do private(s) schedule(static)
   43.      do j=1,nj
   44.         do p=1,nv2
   45.            s = p + (j - 1)*nv2
   46.            x_OUT(j,:,p) = x(:,s)
